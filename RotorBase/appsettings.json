{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_DB_HOST;Port=3306;Database=rotorbase;User Id=YOUR_USER;Password=YOUR_PASSWORD;TreatTinyAsBoolean=true;"
  },
  "App": {
    "BaseUrl": "http://127.0.0.1:5048",
    "InternalBaseUrl": "http://127.0.0.1:5048"
  },
  "Mail": {
    "Provider": "Mailgun",
    "FromEmail": "no-reply@example.com",
    "FromName": "RotorBase",
    "BaseUrl": "http://127.0.0.1:5048",
    "Mailgun": {
      "Domain": "your-mailgun-domain",
      "ApiKey": "YOUR_MAILGUN_API_KEY",
      "BaseUrl": "https://api.mailgun.net"
    }
  },
  "Stripe": {
    "ApiKey": "YOUR_STRIPE_API_KEY",
    "WebhookSecret": "YOUR_STRIPE_WEBHOOK_SECRET",
    "Prices": {
      "PREMIUM": "price_yourPremiumPriceId",
      "STANDARD": "price_yourStandardPriceId"
    }
  },
  "Analytics": {
    "IngestKey": "SET_ME_RANDOM",
    "IpSalt": "SET_ME_DIFFERENT_RANDOM"
  }
}
